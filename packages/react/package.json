    "eslint-config-custom": "*",
    "eslint-plugin-react-hooks": "4.6.0",
    "jsdom": "23.2.0",
    "tsconfig": "*",
    "tsup": "7.2.0",
    "typescript": "5.1.6",
    "viem": "2.0.10",
